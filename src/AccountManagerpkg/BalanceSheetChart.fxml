<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AccountManagerpkg.BalanceSheetChartController">
   <children>
      <BarChart fx:id="balanceSheetChart" layoutX="50.0" layoutY="112.0" prefHeight="465.0" prefWidth="801.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" side="LEFT" />
        </yAxis>
      </BarChart>
      <ComboBox fx:id="selectChartDataTypeCombobox" layoutX="204.0" layoutY="32.0" prefWidth="150.0" />
      <Label layoutX="84.0" layoutY="36.0" text="Select Data Type: " />
      <Button layoutX="514.0" layoutY="32.0" mnemonicParsing="false" onMouseClicked="#showBalanceSheetChart" text="Show Chart" />
      <Button layoutX="757.0" layoutY="32.0" mnemonicParsing="false" onMouseClicked="#backButtonOnClick" text="Back" />
   </children>
</AnchorPane>
